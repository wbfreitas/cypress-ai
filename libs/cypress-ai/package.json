{
  "name": "cypress-ai",
  "version": "0.2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cyai": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/commands": {
      "import": "./dist/commands.js",
      "require": "./dist/commands.js",
      "types": "./dist/commands.d.ts"
    },
    "./src/commands": {
      "import": "./dist/commands.js",
      "require": "./dist/commands.js",
      "types": "./dist/commands.d.ts"
    },
    "./dist/agent": {
      "import": "./dist/agent.js",
      "require": "./dist/agent.js",
      "types": "./dist/agent.d.ts"
    },
    "./src/agent": {
      "import": "./dist/agent.js",
      "require": "./dist/agent.js",
      "types": "./dist/agent.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "dependencies": {
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "dotenv": "^17.2.2",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "cypress": "^12.0.0"
  },
  "keywords": [
    "cypress",
    "ai",
    "testing",
    "e2e",
    "ollama",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
