{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Debug Cypress AI Generate",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/my-angular-app/node_modules/.bin/cypress",
      "args": [
        "run",
        "--spec",
        "cypress/e2e-ai/debug-test.cy.js"
      ],
      "cwd": "${workspaceFolder}/my-angular-app",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/libs/cypress-ai-v2/dist/**/*.js"
      ]
    },
    {
      "name": "Debug TestGenerator",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/libs/cypress-ai-v2/dist/core/TestGenerator.js",
      "cwd": "${workspaceFolder}/my-angular-app",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true
    }
  ]
}
